# Link Shortener Environment Variables
# Copy this file to .env and customize the values

# Database Configuration
POSTGRES_USER=linkuser
POSTGRES_PASSWORD=linkpass
POSTGRES_DB=linkdb

# Admin Credentials (CHANGE THESE IN PRODUCTION!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# JWT Secret (CHANGE THIS IN PRODUCTION!)
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Public Link TTL (default: 48 hours)
PUBLIC_LINK_TTL=48h

# Base URL for generated short links
BASE_URL=https://go.kinter.one

# Frontend Configuration
FRONTEND_URL=https://go.kinter.one
FRONTEND_PORT=3000
